{
  "name": "@npm-telluria-tecnologia/telluria-ui",
  "version": "1.1.21",
  "description": "common react components for application development",
  "main": "dist/index.js",
  "types": "dist",
  "scripts": {
    "test": "react-scripts test --watchAll=false --coverage",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "commit": "git-cz",
    "pubnpm": "yarn build && yarn publish --no-git-tag-version",
    "lint-check": "eslint src --ext js,ts,tsx",
    "lint-fix": "eslint src --fix --ext js,ts,tsx",
    "prepare": "husky install",
    "link-package": "yarn link && npm link ../../Rastrin/Frontend/node_modules/react/",
    "unlink-package": "yarn unlink && npm unlink ../../Rastrin/Frontend/react-app-test-npm-library/node_modules/react/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/jquery": "^3.5.8",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-html-parser": "^2.0.2",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.58.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^7.0.2",
    "jquery": "^3.5.1",
    "react-html-parser": "^2.0.2",
    "react-icons": "^3.11.0",
    "react-scripts": "3.4.4",
    "react-spring": "^8.0.27",
    "styled-components": "^5.2.0",
    "uuidv4": "^6.2.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint-fix",
      "yarn lint-check"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "browserslist": [
    "defaults"
  ]
}
